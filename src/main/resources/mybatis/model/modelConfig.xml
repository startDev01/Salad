		<!-- 하유리(23.07.24.) -->
		<typeAlias type="com.proj.salad.notice.vo.NoticeVO"
			alias="noticeVO" />
			
	 <!-- 조상현 추가  -->	
		<typeAlias type="com.proj.salad.order.vo.AccountVO"
